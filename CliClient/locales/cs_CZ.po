msgstr "Zdrojový formát: %s"

msgid "Do not ask for confirmation."
msgstr "Neptat se na potvrzení."

#, javascript-format
msgid "Found: %d."
msgstr "Nalezeno: %d."

#, javascript-format
msgid "Created: %d."
msgstr "Vytvořeno: %d."

#, javascript-format
msgid "Updated: %d."
msgstr "Upraveno: %d."

#, javascript-format
