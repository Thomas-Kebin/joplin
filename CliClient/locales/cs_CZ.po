msgstr "Přeskočeno: %d."

#, javascript-format
msgid "Resources: %d."
msgstr "Zdroje: %d."

#, javascript-format
msgid "Tagged: %d."
msgstr "Otagováno: %d."

msgid "Importing notes..."
msgstr "Importuji poznámky..."

